{
	"$schema": "https://docs.renovatebot.com/renovate-schema.json",
	"extends": [],
	"forkProcessing": "enabled",
	"branchConcurrentLimit": 1000,
	"prConcurrentLimit": 1000,
	"packageRules": [
		{
			"description": "modify title column for golang.org/x packages",
			"matchManagers": [
				"gomod"
			],
			"prBodyDefinitions": {
				"Package": "{{#if (containsString depName 'golang.org/x/')}}[{{{depName}}}](https://pkg.go.dev/{{{depName}}}){{else}}{{{depNameLinked}}}{{/if}}"
			}
		},
		{
			"matchDepNames": [
				"golang.org/x/*"
			],
			"matchDepTypes": [
				"indirect"
			],
			"enabled": true
		}
	]
}
